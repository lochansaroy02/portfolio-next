{
    "version": 2,
    "builds": [
        {
            "src": "next.config.mjs",
            "use": "@vercel/next"
        }
    ],
    "routes": [
        {
            "src": "/assets/(.*)",
            "dest": "/public/assets/$1"
        },
        {
            "src": "/(.*)",
            "dest": "/$1"
        }
    ],
    "installCommand": "npm install",
    "buildCommand": "npm run build",
    "framework": "nextjs",
    "outputDirectory": "out"
}